<!doctype html>
<html lang="en">
  <head>
      <meta charset="UTF-8">
      <title>Dry Field</title>
      {# <link rel="stylesheet" type="text/css" href="static/css/bootstrap.min.css"> #}
      <link rel="stylesheet/less" type="text/css" href="static/css/style.less" />
      <script src="static/js/lib/less.js" type="text/javascript"></script>
      <script src="/static/js/lib/scrollreveal.min.js" type="text/javascript"></script>

      <link rel="icon" href="static/assets/favicon.png" type="image/png">
  </head>
  <body>
    <div id="game-container">
        <section id="actions">
            {# {% include "wallet.html" %} #}
            <div id="wallet" class="action-child">
                <img src="/static/assets/banque.png" alt=""/>
                <p>
                    0&nbsp;&euro;
                </p>
            </div>

            {# {% include "citern.html" %} #}
            <div id="main-citern" class="action-child">
                <img src="/static/assets/citern.png" alt=""/>
                <p>
                    0&nbsp;L
                </p>
            </div>
        </section>
        <section id="fields">
            <div class="field">
                <img src="/static/assets/field.png" alt="" />
            </div>

            <div class="field">
                <img src="/static/assets/field.png" alt="" />
            </div>

            <div class="field">
                <img src="/static/assets/field.png" alt="" />
            </div>
        </section>
    </section>
    <script>
        window.sr = ScrollReveal({
            'origin': 'bottom',
            'distance': '50px',
            'duration': 1500,
            'reset': false,
            'viewFactor': 0.2
        });
        sr.reveal('#actions > *', null, 250);
        sr.reveal('#fields > *', {
            delay: 500
        }, 250);
    </script>
    <script src="/static/js/app.js" type="text/javascript"></script>
    <!-- <script src="/app/features/wallet/controller.js"></script>
    <script src="/app/features/citern/controller.js"></script>
    <script src="/app/features/score/controller.js"></script>
    <script src="/app/features/field/controller.js"></script> -->
  </body>
</html>