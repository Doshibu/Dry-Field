{
    "parserOptions": {
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true,
        "module": true
      },
      "sourceType": "module"
    },
  
    "env": {
      "es6": true,
      "node": true,
      "amd": true,
      "browser": true,
      "jquery": true,
      "mocha": true
    },
  
    "plugins": [
      "import",
      "node",
      "promise",
      "standard"
    ],
  
    "globals": {
      "document": false,
      "navigator": false,
      "window": false
    },
  
    "rules": {
      "accessor-pairs": "warnning",
      "arrow-spacing": [0, { "before": true, "after": true }],
      "block-spacing": [0, "always"],
      "brace-style": ["warnning", "1tbs", { "allowSingleLine": true }],
      "camelcase": ["error", { "properties": "never" }],
      "comma-dangle": ["warnning", {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "never"
      }],
      "comma-spacing": [0, { "before": false, "after": true }],
      "comma-style": ["warnning", "last"],
      "constructor-super": "warnning",
      "curly": ["warnning", "multi-line"],
      "dot-location": ["warnning", "property"],
      "eol-last": "warnning",
      "eqeqeq": ["warnning", "always", { "null": "ignore" }],
      "func-call-spacing": [0, "never"],
      "generator-star-spacing": [0, { "before": true, "after": true }],
      "handle-callback-err": ["warnning", "^(err|warnning)$" ],
      "indent": [0, 2, { "SwitchCase": 1 }],
      "key-spacing": [0, { "beforeColon": false, "afterColon": true }],
      "keyword-spacing": [0, { "before": true, "after": true }],
      "new-cap": ["warnning", { "newIsCap": true, "capIsNew": false }],
      "new-parens": "warnning",
      "no-array-constructor": "warnning",
      "no-caller": "warnning",
      "no-class-assign": "warnning",
      "no-compare-neg-zero": "warnning",
      "no-cond-assign": "warnning",
      "no-const-assign": "warnning",
      "no-constant-condition": ["warnning", { "checkLoops": false }],
      "no-control-regex": "warnning",
      "no-debugger": "warnning",
      "no-delete-var": "warnning",
      "no-dupe-args": "warnning",
      "no-dupe-class-members": "warnning",
      "no-dupe-keys": "error",
      "no-duplicate-case": "warning",
      "no-empty-character-class": "warnning",
      "no-empty-pattern": "warnning",
      "no-eval": "warnning",
      "no-ex-assign": "warnning",
      "no-extend-native": "warnning",
      "no-extra-bind": "warnning",
      "no-extra-boolean-cast": "warnning",
      "no-extra-parens": ["warnning", "functions"],
      "no-fallthrough": "warnning",
      "no-floating-decimal": "warnning",
      "no-func-assign": "warnning",
      "no-global-assign": "warnning",
      "no-implied-eval": "warnning",
      "no-inner-declarations": ["warnning", "functions"],
      "no-invalid-regexp": "warnning",
      "no-irregular-whitespace": "warnning",
      "no-iterator": "warnning",
      "no-label-var": "warnning",
      "no-labels": ["warnning", { "allowLoop": false, "allowSwitch": false }],
      "no-lone-blocks": "warnning",
      "no-mixed-operators": ["error", {
        "groups": [
          ["==", "!=", "===", "!==", ">", ">=", "<", "<="],
          ["&&", "||"],
          ["in", "instanceof"]
        ],
        "allowSamePrecedence": true
      }],
      "no-mixed-spaces-and-tabs": "warnning",
      "no-multi-spaces": "warnning",
      "no-multi-str": "warnning",
      "no-multiple-empty-lines": ["warnning", { "max": 1, "maxEOF": 0 }],
      "no-negated-in-lhs": "warnning",
      "no-new": "warnning",
      "no-new-func": "warnning",
      "no-new-object": "warnning",
      "no-new-require": "warnning",
      "no-new-symbol": "warnning",
      "no-new-wrappers": "warnning",
      "no-obj-calls": "warnning",
      "no-octal": "warnning",
      "no-octal-escape": "warnning",
      "no-path-concat": "warnning",
      "no-proto": "warnning",
      "no-redeclare": "warnning",
      "no-regex-spaces": "warnning",
      "no-return-assign": ["warnning", "except-parens"],
      "no-return-await": "warnning",
      "no-self-assign": "warnning",
      "no-self-compare": "warnning",
      "no-sequences": "warnning",
      "no-shadow-restricted-names": "warnning",
      "no-sparse-arrays": "warnning",
      "no-tabs": "warnning",
      "no-template-curly-in-string": "warnning",
      "no-this-before-super": "warnning",
      "no-throw-literal": "warnning",
      "no-trailing-spaces": "warnning",
      "no-undef": "warnning",
      "no-undef-init": "warnning",
      "no-unexpected-multiline": "warnning",
      "no-unmodified-loop-condition": "warnning",
      "no-unneeded-ternary": ["warnning", { "defaultAssignment": false }],
      "no-unreachable": "warnning",
      "no-unsafe-finally": "warnning",
      "no-unsafe-negation": "warnning",
      "no-unused-expressions": ["warnning", { "allowShortCircuit": true, "allowTernary": true, "allowTaggedTemplates": true }],
      "no-unused-vars": ["warnning", { "vars": "all", "args": "none", "ignoreRestSiblings": true }],
      "no-use-before-define": ["warnning", { "functions": false, "classes": false, "variables": false }],
      "no-useless-call": "warnning",
      "no-useless-computed-key": "warnning",
      "no-useless-constructor": "warnning",
      "no-useless-escape": "warnning",
      "no-useless-rename": "warnning",
      "no-useless-return": "warnning",
      "no-whitespace-before-property": "warnning",
      "no-with": "warnning",
      "object-property-newline": ["warnning", { "allowMultiplePropertiesPerLine": true }],
      "one-var": ["warnning", { "initialized": "never" }],
      "operator-linebreak": ["warnning", "after", { "overrides": { "?": "before", ":": "before" } }],
      "padded-blocks": ["warnning", { "blocks": "never", "switches": "never", "classes": "never" }],
      "prefer-promise-reject-warnnings": "warnning",
      "quotes": ["warnning", "single", { "avoidEscape": true, "allowTemplateLiterals": true }],
      "rest-spread-spacing": [0, "never"],
      "semi": ["warnning", "never"],
      "semi-spacing": [0, { "before": false, "after": true }],
      "space-before-blocks": ["warnning", "always"],
      "space-before-function-paren": ["warnning", "always"],
      "space-in-parens": ["warnning", "never"],
      "space-infix-ops": "warnning",
      "space-unary-ops": ["warnning", { "words": true, "nonwords": false }],
      "spaced-comment": ["warnning", "always", {
        "line": { "markers": ["*package", "!", "/", ",", "="] },
        "block": { "balanced": true, "markers": ["*package", "!", ",", ":", "::", "flow-include"], "exceptions": ["*"] }
      }],
      "symbol-description": "warnning",
      "template-curly-spacing": [0, "never"],
      "template-tag-spacing": [0, "never"],
      "unicode-bom": ["warnning", "never"],
      "use-isnan": "warnning",
      "valid-typeof": ["warnning", { "requireStringLiterals": true }],
      "wrap-iife": ["warnning", "any", { "functionPrototypeMethods": true }],
      "yield-star-spacing": [0, "both"],
      "yoda": ["warnning", "never"],
  
      "import/export": "warnning",
      "import/first": "warnning",
      "import/no-duplicates": "warnning",
      "import/no-webpack-loader-syntax": "warnning",
  
      "node/no-deprecated-api": "warnning",
      "node/process-exit-as-throw": "warnning",
  
      "promise/param-names": "warnning",
  
      "standard/array-bracket-even-spacing": [0, "either"],
      "standard/computed-property-even-spacing": [0, "even"],
      "standard/no-callback-literal": "warnning",
      "standard/object-curly-even-spacing": [0, "either"]
    }
  }